<%- include('./parts/starthtml') %>
<%- include('./parts/header') %>

<div class="container">
  <table class="grid">
    <tr class="titulos">
      <th>Nome</th>
      <th>Email</th>
      <th colspan="3">Ações</th>
    </tr>
    <% usuarios.forEach(usuario => { %>
      <tr class="conteudo">
        <td><%= usuario.nome %></td>
        <td><%= usuario.email %></td>
        <td>
          <a href="/ver/<%= usuario.id %>/"><button class="ver">Ver</button></a>
          <a href="/editar/<%= usuario.id %>"><button class="editar">Editar</button></a>
          <a href="/excluir/<%= usuario.id %>"><button class="excluir">Excluir</button></a>
        </td>
      </tr>
    <% }) %>
  </table>
</div>

<%- include('./parts/footer') %>
<%- include('./parts/endhtml') %>